server {
        server_name _;
        server_name_in_redirect off;
        root /var/www;
    location ~ \.php$ {
            fastcgi_pass unix:/var/run/php5-fpm.sock;
            fastcgi_index index.php;
            include fastcgi_params;
    }
}

server {
        server_name js-agent.newrelic.com;
        root /var/www/cs/js-agent.newrelic.com;
}

server {
        server_name fonts.googleapis.com;
        root /var/www/cs/fonts.googleapis.com;

      location /css {
            try_files $uri /css.php?$args;
      }
    location ~ \.php$ {
            fastcgi_pass unix:/var/run/php5-fpm.sock;
            fastcgi_index index.php;
            include fastcgi_params;
    }
}

server {
        server_name www.google.com;
        root /var/www/cs/www.google.com;
}
    
server {
        server_name www.cs-first.com;
        root /var/www/cs/www.cs-first.com;
}

server {
        server_name cs-first.com;
        root /var/www/cs/cs-first.com;
}

server {
        server_name files.cs-first.com;
        root /var/www/cs/files.cs-first.com;
}

server {
        server_name scratch.mit.edu;
        root /var/www/scratch;

location / {
            try_files $uri $uri/ /cloud.php?$args;
}

    location ~ \.php$ {
            fastcgi_pass unix:/var/run/php5-fpm.sock;
            fastcgi_index index.php;
            include fastcgi_params;
    }
}

server {
        server_name www.scratch.mit.edu;
        root /var/www/scratch;

location / {
            try_files $uri $uri/ /cloud.php?$args;
}

    location ~ \.php$ {
            fastcgi_pass unix:/var/run/php5-fpm.sock;
            fastcgi_index index.php;
            include fastcgi_params;
    }
}
